#include<stdio.h>
int main()
{
int num[25];
int a,j,i,n;
printf("enter total no of elements:");
scanf("%d",&n);
printf("enter numbers one by one:");
for(i=0;i<n;++i)
{
scanf("%d",&num[i]);
}
for(i=0;i<n;++i)
{
for(j=i+1;j<n;++j)
{
if(num[i]>num[j])
{
a=num[i];
num[i]=num[j];
num[j]=a;
}
}
}
printf("numbers in assending order:");
for(i=0;i<nl\;++i)
{
printf("%d\n",num[i]);
}
return 0;
}
